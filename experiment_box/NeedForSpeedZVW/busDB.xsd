<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:element name="bustypeDB">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bus">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="producer">
                                <xs:simpleType>

                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="zeidel-dieselModel">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="SL 200"/>
                                        <xs:enumeration value="SG 192"/>
                                        <xs:enumeration value="SL 202"/>
                                        <xs:enumeration value="SG 242"/>
                                        <xs:enumeration value="NL 202"/>
                                        <xs:enumeration value="NG 272"/>
